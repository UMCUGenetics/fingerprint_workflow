executor {
    name = 'local'
    queueSize = 1
}

params {
    genome = "fingerprint_workflow/files/genome/Homo_sapiens.GRCh37.GATK.illumina.fasta"

    // MIPS trim dedup
    design_file = "fingerprint_workflow/files/fingerprint/81_snp_mips_design.txt"
    uuid_length = 8
    uuid_read = "R1"

    // GATK UnifiedGenotyper
    target_intervals = "fingerprint_workflow/files/fingerprint/81SNP_design.vcf"
    dbsnp = "fingerprint_workflow/files/fingerprint/81SNP_design.vcf"
}
